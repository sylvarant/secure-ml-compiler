#########################################


#============================================
# Tools
#============================================

OCAMLBUILD=ocamlbuild


#============================================
# Executable Targets
#============================================

TARGET=main


#============================================
# Compilation sources
#============================================

SOURCES =$(wildcard *.ml*)


#============================================
# Linking
#============================================

native:
	$(OCAMLBUILD) $(TARGET).native


#============================================
# Results
#============================================
all: native


clean:
	$(OCAMLBUILD) -clean
